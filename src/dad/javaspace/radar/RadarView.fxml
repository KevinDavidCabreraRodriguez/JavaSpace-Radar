   <children>
      <Pane fx:id="radarPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" styleClass="radarPane" VBox.vgrow="ALWAYS">
         <children>
            <Polygon fx:id="playerTriangle" fill="DODGERBLUE" scaleX="0.1" scaleY="0.1" stroke="BLACK" strokeType="INSIDE">
              <points>
                <Double fx:value="-50.0" />
                <Double fx:value="40.0" />
                <Double fx:value="50.0" />
                <Double fx:value="40.0" />
                <Double fx:value="0.0" />
                <Double fx:value="-60.0" />
              </points>
            </Polygon>
         </children>
         <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </VBox.margin>
      </Pane>
   </children>
</fx:root>
